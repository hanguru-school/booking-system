{
  "name": "booking-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000 --hostname 0.0.0.0",
    "dev:turbo": "next dev --turbo --port 3000 --hostname 0.0.0.0",
    "build": "prisma generate && next build",
    "start": "next start",
    "start:nas": "next start -p ${PORT:-3000} -H ${HOSTNAME:-0.0.0.0}",
    "lint": "eslint . --ext .ts,.tsx",
    "fix-unused": "eslint . --ext .ts,.tsx --fix",
    "vercel-build": "prisma generate && next build",
    "nas-build": "prisma generate && next build && echo 'NAS 빌드 완료'",
    "nas-deploy": "npm run nas-build && npm run start:nas",
    "optimize": "node scripts/optimize.js",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build -- --analyze",
    "postinstall": "prisma generate",
    "check-production": "node scripts/production-check.js",
    "production-setup": "npm run check-production && echo '운영 서버 설정을 확인하세요: http://192.168.1.41:3000'",
    "setup-branch-protection": "./scripts/setup-branch-protection.sh",
    "protect-main": "npm run setup-branch-protection"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.855.0",
    "@aws-sdk/client-s3": "^3.857.0",
    "@prisma/client": "^6.12.0",
    "@types/archiver": "^6.0.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.15.4",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "amazon-cognito-identity-js": "^6.3.15",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.21",
    "aws-sdk": "^2.1692.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "firebase": "^12.0.0",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.525.0",
    "next": "15.4.1",
    "nodemailer": "^6.9.16",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "prisma": "^6.12.0",
    "puppeteer": "^24.26.1",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-qr-code": "^2.0.18",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-next": "15.4.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.6.2",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
